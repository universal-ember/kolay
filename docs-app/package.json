{
  "name": "docs-app",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "imports": {
    "#config": "./src/config.ts",
    "#components/*": "./src/components/*",
    "#routes/*": "./src/routes/*"
  },
  "exports": {
    "./tests/*": "./tests/*",
    "./*": "./src/*"
  },
  "scripts": {
    "build:app": "vite build",
    "lint": "pnpm -w turbo --filter docs-app _:lint",
    "lint:fix": "pnpm -w turbo --filter docs-app _:lint:fix",
    "lint:types": "glint",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:prettier": "prettier . --check",
    "lint:prettier:fix": "prettier . --write",
    "start": "pnpm start:vite",
    "start:vite": "vite",
    "start:test": "pnpm start:vite --open /tests/",
    "start:sync": "pnpm _syncPnpm --watch",
    "_syncPnpm": "DEBUG=* pnpm sync-dependencies-meta-injected",
    "test:ember": "vite build --mode test && pnpm dist:ember:test",
    "dist:ember:test": "ember test --path dist --config-file ./testem.cjs"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/plugin-proposal-decorators": "^7.28.0",
    "@ember/app-tsconfig": "^2.0.0",
    "@ember/optional-features": "^2.3.0",
    "@ember/string": "^4.0.1",
    "@ember/test-helpers": "^5.4.1",
    "@ember/test-waiters": "^4.0.0",
    "@embroider/compat": "^4.1.11",
    "@embroider/core": "^4.4.1",
    "@embroider/macros": "^1.19.5",
    "@embroider/vite": "^1.4.4",
    "@glimmer/component": "^2.0.0",
    "@glint/ember-tsc": "1.0.8",
    "@glint/template": "1.7.3",
    "@glint/tsserver-plugin": "2.0.8",
    "@nullvoxpopuli/eslint-configs": "^5.0.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@types/qunit": "^2.19.13",
    "babel-plugin-ember-template-compilation": "^3.0.1",
    "concurrently": "^9.2.1",
    "decorator-transforms": "^2.3.0",
    "ember-auto-import": "^2.12.0",
    "ember-cli": "^6.9.1",
    "ember-eslint": "^0.6.1",
    "ember-modifier": "^4.2.0",
    "ember-page-title": "^9.0.3",
    "ember-primitives": "^0.48.2",
    "ember-qunit": "^9.0.4",
    "ember-resize-observer-service": "^1.1.0",
    "ember-resolver": "^13.1.0",
    "ember-resources": "^7.0.4",
    "ember-source": "6.5.0-alpha.6",
    "ember-template-lint": "^7.9.3",
    "eslint": "^9.39.2",
    "loader.js": "^4.7.0",
    "pnpm-sync-dependencies-meta-injected": "^0.0.15",
    "prettier": "^3.7.4",
    "prettier-plugin-ember-template-tag": "^2.1.2",
    "qunit": "^2.24.1",
    "qunit-dom": "^3.5.0",
    "read-package-up": "^12.0.0",
    "tracked-built-ins": "^4.0.0",
    "typescript": "^5.9.3",
    "unplugin-info": "^1.2.4",
    "vite": "^7.3.0",
    "vite-plugin-wasm": "^3.5.0"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "type": "app",
    "version": 2
  },
  "volta": {
    "extends": "../package.json"
  },
  "dependencies": {
    "@shikijs/rehype": "^3.20.0",
    "@universal-ember/test-support": "^0.5.1",
    "change-case": "^5.4.3",
    "content-tag": "^4.0.0",
    "ember-async-data": "^2.0.0",
    "ember-cached-decorator-polyfill": "^1.0.2",
    "ember-mobile-menu": "^6.0.0",
    "ember-repl": "7.1.0",
    "ember-route-template": "^1.0.3",
    "kolay": "workspace:^",
    "reactiveweb": "^1.9.0",
    "shiki": "^3.20.0"
  }
}

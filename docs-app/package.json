    "ember-cached-decorator-polyfill": "^1.0.2",
    "ember-mobile-menu": "^5.1.0",
    "ember-repl": "5.0.1",
    "ember-route-template": "^1.0.3",
    "kolay": "workspace:^",
    "reactiveweb": "^1.4.0",
    "shiki": "^3.0.0"
  }
}

{
  "name": "kolay",
  "version": "0.0.3",
  "description": "",
  "keywords": [
    "documentation",
    "docs",
    "docgen",
    "generator",
    "build",
    "plugin",
    "plugins",
    "ember",
    "ember-addon"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:universal-ember/kolay.git"
  },
  "license": "MIT",
  "author": "NullVoxPopuli",
  "type": "module",
  "exports": {
    ".": {
      "default": "./dist/browser/index.js"
    },
    "./addon-main.cjs": {
      "require": "./addon-main.cjs"
    },
    "./build": {
      "import": "./src/plugins/index.js"
    },
    "./build/legacy": {
      "default": "./src/plugins/legacy/index.cjs"
    }
  },
  "ember-addon": {
    "version": 2,
    "type": "addon",
    "main": "addon-main.cjs"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "prepack": "pnpm build",
    "build": "rollup -c",
    "fix": "pnpm lint:fix && pnpm --filter docs-app lint:fix",
    "lint": "concurrently 'npm:lint:*(!fix)' --names 'lint:'",
    "lint:fix": "pnpm lint:js:fix && pnpm lint:prettier:fix",
    "lint:js": "eslint .",
    "lint:js:fix": "eslint . --fix",
    "lint:package": "publint",
    "lint:prettier": "prettier . --check",
    "lint:prettier:fix": "prettier . --write",
    "lint:published-types": "attw --pack",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@embroider/addon-shim": "^1.8.7",
    "decorator-transforms": "^1.1.0",
    "globby": "^14.0.0",
    "tracked-built-ins": "^3.3.0",
    "unplugin": "^1.6.0"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.13.5",
    "@embroider/addon-dev": "^4.1.3",
    "@nullvoxpopuli/eslint-configs": "^3.2.2",
    "@rollup/plugin-babel": "^6.0.4",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "babel-plugin-ember-template-compilation": "^2.2.1",
    "concurrently": "^8.2.2",
    "ember-hbs-minifier": "^1.2.0",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1",
    "publint": "^0.2.7",
    "release-plan": "^0.6.0",
    "rollup": "^4.9.6",
    "typescript": "^5.3.3"
  },
  "volta": {
    "node": "20.10.0",
    "pnpm": "8.13.1"
  }
}

{
  "name": "markdown-and-gjs-md-app",
  "version": "0.0.0",
  "private": true,
  "description": "Your Ember App's description here",
  "repository": "",
  "license": "MIT",
  "author": "",
  "type": "module",
  "imports": {
    "#app/*": "./app/*",
    "#config": "./app/config.ts",
    "#components/*": "./app/components/*",
    "#services/*": "./app/services/*",
    "#test-helpers/*": "./tests/helpers/*",
    "#utils/*": "./app/utils/*"
  },
  "exports": {
    "./tests/*": "./tests/*",
    "./*": "./app/*"
  },
  "scripts": {
    "dev": "vite",
    "start": "vite",
    "lint:eslint": "eslint . --cache",
    "lint:eslint:fix": "eslint . --fix",
    "lint": "concurrently \"pnpm:lint:*(!fix)\" --names \"lint:\" --prefixColors auto",
    "lint:fix": "concurrently \"pnpm:lint:*:fix\" --names \"fix:\" --prefixColors auto && pnpm format",
    "format": "prettier --write .",
    "lint:prettier": "prettier --check .",
    "test:ember": "pnpm build:test && pnpm test:ci",
    "build:test": "vite build --mode development",
    "test:ci": "testem ci --port 0",
    "test": "pnpm build:test && pnpm test:ci",
    "lint:types": "ember-tsc --noEmit"
  },
  "dependencies": {
    "@embroider/macros": "1.19.7",
    "@embroider/router": "3.0.6",
    "@glimmer/component": "^2.0.0",
    "decorator-transforms": "2.3.1",
    "ember-source": "6.10.1",
    "ember-strict-application-resolver": "0.1.1"
  },
  "devDependencies": {
    "@babel/core": "7.29.0",
    "@babel/plugin-transform-runtime": "7.29.0",
    "@babel/plugin-transform-typescript": "7.28.6",
    "@babel/runtime": "7.28.6",
    "@ember/app-tsconfig": "2.0.0",
    "@ember/test-helpers": "5.4.1",
    "@ember/test-waiters": "^4.0.0",
    "@embroider/core": "4.4.3",
    "@embroider/vite": "1.5.1",
    "@glint/ember-tsc": "1.1.1",
    "@glint/template": "1.7.4",
    "@glint/tsserver-plugin": "2.1.0",
    "@nullvoxpopuli/eslint-configs": "5.5.0",
    "@rollup/plugin-babel": "6.1.0",
    "@types/qunit": "2.19.13",
    "@universal-ember/test-support": "^0.5.1",
    "babel-plugin-ember-template-compilation": "4.0.0",
    "concurrently": "9.2.1",
    "ember-qunit": "9.0.4",
    "eslint": "9.39.2",
    "kolay": "workspace:*",
    "prettier": "3.8.1",
    "prettier-plugin-ember-template-tag": "2.1.3",
    "qunit": "2.25.0",
    "qunit-dom": "3.5.0",
    "qunit-theme-ember": "1.0.0",
    "testem": "3.17.0",
    "typescript": "5.9.3",
    "vite": "8.0.0-beta.11"
  },
  "engines": {
    "node": ">= 24"
  },
  "ember": {
    "edition": "octane"
  }
}

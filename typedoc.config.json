{
  "$schema": "https://typedoc.org/schema.json",
  "tsconfig": "./tsconfig.typedoc.json",
  "entryPoints": ["./declarations/**"],
  "exclude": ["node_modules"],
  "cleanOutputDir": true,
  "compilerOptions": {
    "noEmitOnError": false
  },
  "out": "./dist-typedoc",
  "json": "./dist-typedoc/typedoc.json",
  "pretty": true,
  "excludeInternal": false,
  "skipErrorChecking": true,
  "plugin": []
}
